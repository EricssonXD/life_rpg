{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "dart",
			"args": [
				"run",
				"build_runner",
				"watch",
				"--delete-conflicting-outputs"
			],
			"problemMatcher": [
				"$dart-build_runner"
			],
			"group": {
				"kind": "build",
